@model IEnumerable<User>
@{
    Layout = "_Layout";
    ViewBag.Title = "İstifadəçilərin siyahısı";
}


<section class="content-header">
    <ol class="breadcrumb">
        <li>
            <a asp-controller="Home" asp-action="Index"><i class="fa fa-dashboard"></i> Əsas</a>
        </li>
        <li class="active">İstifadəçilərin siyahısı</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <!-- Default box -->
    <div class="box">

        <!-- /.box-header -->
        <div class="box-body">
            <h2>İstifadəçilərin siyahısı</h2>
            <table id="example1" class="table table-bordered table-striped">
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.Email</td>
                        <td>
                            <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Giriş hüququ</a>
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</section>